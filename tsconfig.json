{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "baseUrl": "./",
    "outDir": "./dist",
    "rootDir": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "noImplicitAny": false,
    "strictPropertyInitialization": false
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "dist", "./src/db/models"]
}
