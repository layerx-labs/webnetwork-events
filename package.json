  "bugs": {
    "url": "https://github.com/moshmage/webnetwork-events/issues"
  },
  "homepage": "https://github.com/moshmage/webnetwork-events#readme",
  "dependencies": {
    "@taikai/dappkit": "^2.0.6",
    "cron-module-actions": "^0.1.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "ipfs-http-client": "^57.0.3",
    "node-html-to-image": "^3.2.4",
    "octokit": "^2.0.4",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "sequelize-auto": "^0.8.8"
  },
  "devDependencies": {
    "@octokit/types": "^6.41.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
